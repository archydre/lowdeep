{
  "name": "lowdeep",
  "version": "1.1.7",
  "description": "The fluent, resilient, and 100% Type-Safe AI SDK with Self-Healing logic.",
  "keywords": [
    "ai",
    "openai",
    "groq",
    "zod",
    "type-safe",
    "builder"
  ],
  "homepage": "https://github.com/archydre/lowdeep#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archydre/lowdeep.git"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist && bun build ./src/index.ts --outdir ./dist --target node --external '*' && tsc"
  },
  "author": "Zaqueu Nilton de Souza Paiva <zaqueunilton1@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "openai": "^6.18.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
